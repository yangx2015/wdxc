<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ldz.biz.module.mapper.ClGpsLsMapper">

<select id="getZdbhAllLsGps" parameterType="com.ldz.biz.module.bean.gpsSJInfo" resultType="com.ldz.biz.module.model.ClGpsLs">
select * from CL_GPS_LS t where   t.CJSJ between to_date(#{startTime},'yyyy-mm-dd hh24:mi:ss') 
 and to_date(#{endTime},'yyyy-mm-dd hh24:mi:ss') and ZDBH=#{zdbh} ORDER BY CJSJ ASC 
</select>

